<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CommonToolBar">
        <attr name="title" format="string"/>
    </declare-styleable>


    <declare-styleable name="StatusLayout">
        <attr name="view_loading" format="reference"/>
        <attr name="view_error" format="reference"/>
        <attr name="load_default" format="enum">
            <enum name="loading" value="0"/>
            <enum name="error" value="1"/>
            <enum name="empty" value="2"/>
            <enum name="content" value="3"/>
        </attr>
        <attr name="view_empty" format="reference"/>

    </declare-styleable>
</resources>